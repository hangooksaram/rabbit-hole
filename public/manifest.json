{
  "manifest_version": 3,
  "name": "Rabbit Hole",
  "version": "1.0.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "/js/background.js",
    "type": "module"
  },
  "icons": {
    "16": "img/default-icon.png",
    "48": "img/default-icon.png",
    "128": "img/default-icon.png"
  },
  "homepage_url": "https://github.com/hangooksaram/rabbit-hole"
}
