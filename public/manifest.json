{
  "manifest_version": 3,
  "name": "Rabbit Hole",
  "version": "1.0.0",
  "description": "Chrome Extension with TypeScript",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting", "storage", "history"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "/js/background.js",
    "type": "module"
  }
}
